{
    "extends": "recommended",
    "rules": {
        "ClassJavadoc": {
            "enabled": false
        },
        "CompileStatic": {
            "enabled": false
        },
        "DuplicateStringLiteral": {
            "enabled": false
        },
        "LineLength": {
            "enabled": false
        },
        "NoWildcardImports": {
            "enabled": false
        },
        "UnnecessaryObjectReferences": {
            "enabled": false
        },
        "UnnecessaryPublicModifier": {
            "enabled": false
        }
    }
}